uri = redis:host=172.18.0.3:port=6379

default_pass_scheme = BLF-CRYPT

# Passdb configuration
key passdb {
  key = user:%u
  format = json
}

# Userdb configuration
key userdb {
  key = user:%u
  format = json
}

# Disable iteration if not needed
iterate_disable = yes

passdb_objects = passdb
userdb_objects = userdb
